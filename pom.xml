	<build>
		<finalName>LicensePoolLifeCycle</finalName>
		<plugins>
			<plugin>
				 <artifactId>maven-compiler-plugin</artifactId>
				 <configuration>
					<source>1.6</source>
					<target>1.6</target>
					</configuration>
			 </plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>			
				<executions>
					<execution>
						<id>unpack</id>
						<phase>initialize</phase>
						<goals>
							<goal>unpack</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<artifactItems>
						<artifactItem>
							<groupId>com.pearson.ed.contract</groupId>
							<artifactId>rumba-contracts</artifactId>
							<version>1.0.0</version>
							<type>jar</type>
							<overWrite>true</overWrite>
							<outputDirectory>target/contract</outputDirectory>
						</artifactItem>
					</artifactItems>
				</configuration>
			</plugin>
			<!--The maven-antrun-plugin is used to copy Spring applicationContext
				files into the target/classes directory-->
			<plugin>
				<artifactId>maven-antrun-plugin</artifactId>
				<executions>
					<execution>
						<id>copy-spring-application-context</id>
						<phase>process-resources</phase>
						<configuration>
							<tasks>
								<copy todir="target/classes"
									failonerror="false" overwrite="true">
									<fileset dir="target/contract/licensepool"/>
									<fileset dir="target/contract/common">
										<include name="Service.xsd"/>
									</fileset>
									<fileset dir="target/contract/organization">
										<include name="Organization_2009_07_01.xsd"/>
									</fileset>
								</copy>
								<copy todir="target/LicensePoolLifeCycle"
									failonerror="false" overwrite="true">
									<fileset dir="target/contract/licensepool"/>
									<fileset dir="target/contract/common">
										<include name="Service.xsd"/>
									</fileset>
									<fileset dir="target/contract/organization">
										<include name="Organization_2009_07_01.xsd"/>
									</fileset>
								</copy>
							</tasks>
						</configuration>
						<goals>
							<goal>run</goal>
						</goals>
					</execution>
				</executions>
			</plugin>

		  <plugin>
			<groupId>org.jvnet.jaxb2.maven2</groupId>
			<artifactId>maven-jaxb2-plugin</artifactId>
			<executions>
				<execution>
					<phase>generate-sources</phase>
					<goals>
						<goal>generate</goal>
					</goals>
				</execution>
			</executions>
			<configuration>
                  <generatePackage>com.pearson.ed.lplc.ws.schema</generatePackage>
					<schemaDirectory>target/contract</schemaDirectory>
					<catalog>target/contract/jaxbcatalog.cat</catalog>
					<schemaIncludes>
						<include>licensepool/*.xsd</include>
						<include>organization/Organization_2009_07_01.xsd</include>
					</schemaIncludes>
				  <strict>true</strict>
              </configuration>
		</plugin>

			<plugin>
				<groupId>org.mortbay.jetty</groupId>
				<artifactId>maven-jetty-plugin</artifactId>
				<version>6.1.10</version>
				<dependencies>
					<dependency>
						<groupId>javax.activation</groupId>
						<artifactId>activation</artifactId>
						<version>1.1</version>
					</dependency>
					<dependency>
